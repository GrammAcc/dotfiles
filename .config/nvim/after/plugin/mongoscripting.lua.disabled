vim.keymap.set("v", "<leader>mr", require("mongoscripting").run_selection)
vim.keymap.set("n", "<leader>mr", require("mongoscripting").run_buffer)
vim.keymap.set("n", "<C-8>", require("mongoscripting").next_db)
vim.keymap.set("n", "<C-7>", require("mongoscripting").prev_db)
vim.keymap.set("n", "<C-S-8>", require("mongoscripting").next_uri)
vim.keymap.set("n", "<C-S-7>", require("mongoscripting").prev_uri)
vim.keymap.set({"v","n"}, "<leader>mt", require("mongoscripting").toggle_ui)
vim.keymap.set({"v", "n", "i"}, "<leader>mep", require("mongoscripting").put_input)
vim.keymap.set({"v", "n", "i"}, "<leader>mwp", require("mongoscripting").put_output)
